
<h2>Voters</h2>
<table id="table">
  <tbody>
    <% @vote_cards.each do |vote_card| %>
    <tr id="row1">
      <td id="column1"><%= vote_card.name %></td>
      <td id="columncrud"><%= link_to 'Show', vote_card %></td>
      <td id="columncrud"><%= link_to 'Edit', edit_vote_card_path(vote_card) %></td>
      <td id="columncrud"><%= link_to 'Destroy', vote_card, method: :delete, data: { confirm: 'Are you sure?' } %></td>
    </tr>
    <% end %>
  </tbody>
</table>
<br>
<%= link_to 'New Vote Card', new_vote_card_path %>
